<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
  <policies>
    <policy id="connect-src">
      <values>
        <value id="experian-marketing-services" type="host">api.experianmarketingservices.com</value>
        <value id="edq-pro-web" type="host">*.qas.com</value>
        <value id="edq-api" type="host">api.edq.com</value>

      </values>
    </policy>
    <policy id="style-src">
      <values>
        <value id="edq-style" type="host">edqprofservus.blob.core.windows.net</value>
      </values>
    </policy>
    <policy id="script-src">
      <values>
        <value id="edq" type="host">edqprofservus.blob.core.windows.net</value>
        <value id="localhost" type="host">api.experianmarketingservices.com</value>
        <value id="localhost-self" type="host">'self'</value>
      </values>
    </policy>
    <policy id="default-src">
      <values>
        <value id="localhost-d" type="host">localhost:8000</value>
        <value id="localhost-self-d" type="host">'self'</value>
      </values>
    </policy>
  </policies>
</csp_whitelist>
